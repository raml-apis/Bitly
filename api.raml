resourceTypes:
  - base: !include resourceTypes/base.raml
securedBy: [ oauth_2_0 ]
/highvalue: 
  type: base
  get: 
    description: |
      Returns a specified number of "high-value" bitly links that are popular
      across bitly at this particular moment.
      
      Deprecated endpoint.
    queryParameters: 
      limit: 
        description: The maximum number of high-value links to return.
        type: integer
    responses: 
      200: 
        body: 
          application/json: 
            example: !include examples/highvalue-example.json
/search: 
  type: base
  get: 
    description: |
      Search links receiving clicks across bitly by content, language, location,
      and more.
      
      Deprecated endpoint.
    queryParameters: 
      limit: 
        description: The maximum number of high-value links to return.
        type: integer
      offset: 
        description: Which result to start with
        type: integer
        default: 0
      query: 
        description: String to query for.
        required: true
      lang: 
        description: Favor results in this language (two letter ISO code).
        maxLength: 2
      cities: 
        description: |
          Show links active in this city (ordered like country-state-city, e.g.
          us-il-chicago).
      domain: 
        description: Restrict results to this web domain.
      fields: 
        description: |
          Which fields to return in the response (comma-separated).
          By default, all will be returned.
        enum: 
          - domain
          - initial_epoch
          - h2
          - h3
          - site
          - lastindexed
          - keywords
          - last_indexed_epoch
          - title
          - initial
          - summaryText
          - content
          - score
          - summaryTitle
          - type
          - description
          - cities
          - lang
          - url
          - referrer
          - aggregate_link
          - lastseen
          - page
          - ogtitle
    responses: 
      200: 
        body: 
          application/json: 
            example: !include examples/search-example.json
/realtime: 
  /bursting_phrases: 
    type: base
    get: 
      description: |
        Returns phrases that are receiving an uncharacteristically high volume of
        click traffic, and the individual links (hashes) driving traffic to pages
        containing these phrases.
        
        Deprecated endpoint.
      responses: 
        200: 
          body: 
            application/json: 
              example: !include examples/realtime-bursting_phrases-example.json
  /hot_phrases: 
    type: base
    get: 
      description: |
        Returns phrases that are receiving a consistently high volume of click
        traffic, and the individual links (hashes) driving traffic to pages
        containing these phrases.
        
        Deprecated endpoint.
      responses: 
        200: 
          body: 
            application/json: 
              example: !include examples/realtime-hot_phrases-example.json
  /clickrate: 
    type: base
    get: 
      description: |
        Returns the click rate for content containing a specified phrase.
        
        Deprecated endpoint.
      queryParameters: 
        phrase: 
          description: "The phrase for which you'd like to get the click rate."
      responses: 
        200: 
          body: 
            application/json: 
              example: !include examples/realtime-clickrate-example.json
/link: 
  /language: 
    type: base
    get: 
      description: |
        Returns the significant languages for the bitly link. Note that languages
        are highly dependent upon activity (clicks) occurring on the bitly link.
        If there have not been clicks on a bitly link within the last 24 hours, i
        is possible that language data for that link does not exist.
        
        Deprecated endpoint.
      queryParameters: 
        link: 
          description: A bitly link.
      responses: 
        200: 
          body: 
            application/json: 
              example: !include examples/link-language.json
  /content: 
    type: base
    get: 
      description: |
        Returns the "main article" from the linked page, as determined by the conten
        extractor, in either HTML or plain text format.
        
        Deprecated endpoint.
      queryParameters: 
        link: 
          description: A bitly link.
          required: true
        content_type: 
          description: |
            Specifies whether to return the content as html or plain text.
          enum: [ html , plain_text ]
          default: html
      responses: 
        200: 
          body: 
            application/json: 
              example: !include examples/link-content-example.json
  /shares: 
    type: base
    get: 
      description: |
        Returns metrics about a shares of a single link.
      queryParameters: 
        link: 
          description: A bitly link.
        unit: 
          description: |
            minute, hour, day, week or month, default: day
            Note: when unit is minute the maximum value for units is 60.
          enum: 
            - minute
            - hour
            - day
            - week
            - month
          default: day
        units: 
          description: |
            An integer representing the time units to query data for. Pass -1 to
            return all units of time.
          type: integer
        timezone: 
          description: |
            An integer hour offset from UTC (-14 to 14), or a timezone string
            default: America/New_York.
          type: integer
          default: -4
        rollup: 
          description: |
            Return data for multiple units rolled up to a single result instead of
            a separate value for each period of time.
          type: boolean
        limit: 
          description: 1 to 1000
          type: integer
          default: 100
        unit_reference_ts: 
          description: |
            An epoch timestamp, indicating the most recent time for which to pull
            metrics, default: now.
            The value of unit_reference_ts rounds to the nearest unit.
      responses: 
        200: 
          body: 
            application/json: 
              schema: link-shares
              example: !include examples/link-shares-example.json
  /encoders: 
    type: base
    get: 
      description: |
        Returns users who have encoded this link (optionally only those in the
        requesting user's social graph).
        Note: Some users may not be returned from this call depending on link
        privacy settings.
      queryParameters: 
        link: 
          description: A bitly link.
          required: true
        my_network: 
          description: True or false - restrict to my network.
          type: boolean
        limit: 
          description: |
            Integer in the range of 1 to 25 that specifies the number of records
            to return.
          type: integer
          default: 10
        expand_user: 
          description: True or false - include display names of encoders.
          type: boolean
      responses: 
        200: 
          body: 
            application/json: 
              schema: link-encoders
              example: !include examples/link-encoders-example.json
  /category: 
    type: base
    get: 
      description: |
        Returns the detected categories for a document, in descending order of
        confidence.
        
        Deprecated endpoint.
      queryParameters: 
        link: 
          description: A bitly link.
      responses: 
        200: 
          body: 
            application/json: 
              example: !include examples/link-category-example.json
  /info: 
    type: base
    get: 
      description: |
        Returns metadata about a single bitly link.
      queryParameters: 
        link: 
          description: A bitly link.
      responses: 
        200: 
