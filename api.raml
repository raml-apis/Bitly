#%RAML 0.8
title: Bitly
version: v3
baseUri: https://api-ssl.bitly.com/{version}/
mediaType: application/json
securitySchemes:
  - oauth_2_0: !include securitySchemes/oauth_2_0.raml
schemas:
  - link-shares: !include schemas/link-shares-schema.json
  - link-encoders: !include schemas/link-encoders-schema.json
  - link-countries: !include schemas/link-countries-schema.json
  - link-encoders_count: !include schemas/link-encoders_count-schema.json
  - link-clicks: !include schemas/link-clicks-schema.json
  - link-referrers: !include schemas/link-referrers-schema.json
  - link-referrers-schema: !include schemas/link-referrers-schema.json
  - link-lookup: !include schemas/link-lookup-schema.json
  - link-referring_domains: !include schemas/link-referring_domains-schema.json
  - expand: !include schemas/expand-schema.json
  - user-network_history: !include schemas/user-network_history-schema.json
  - user-tracking_domain_clicks: !include schemas/user-tracking_domain_clicks-schema.json
  - user-shorten_counts: !include schemas/user-shorten_counts-schema.json
  - user-info-enterprise_user: !include schemas/user-info-enterprise_user-schema.json
  - user-share_counts: !include schemas/user-share_counts-schema.json
  - user-countries: !include schemas/user-countries-schema.json
  - user-link_history: !include schemas/user-link_history-schema.json
  - user-popular_links: !include schemas/user-popular_links-schema.json
  - user-tracking_domain_shorten_counts: !include schemas/user-tracking_domain_shorten_counts-schema.json
  - user-clicks: !include schemas/user-clicks-schema.json
  - user-link_save: !include schemas/user-link_save-schema.json
  - user-referrers: !include schemas/user-referrers-schema.json
  - user-tracking_domain_list: !include schemas/user-tracking_domain_list-schema.json
  - user-referring_domains: !include schemas/user-referring_domains-schema.json
  - user-link_edit: !include schemas/user-link_edit-schema.json
  - user-link_lookup: !include schemas/user-link_lookup-schema.json
  - info: !include schemas/info-schema.json
  - shorten: !include schemas/shorten-schema.json
  - bundle-clone: !include schemas/bundle-clone-schema.json
  - bundle-clone-schema: !include schemas/bundle-clone-schema.json
  - bundle-view_count: !include schemas/bundle-view_count-schema.json
