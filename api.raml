  /lookup: 
    type: base
    get: 
      description: |
        This is used to query for a bitly link based on a long URL.
      queryParameters: 
        url: 
          description: One or more long URLs to lookup.
      responses: 
        200: 
          body: 
            application/json: 
              schema: link-lookup
              example: !include examples/link-lookup-example.json
