  /countries: 
    type: base
    get: 
      description: "Returns aggregate metrics about the countries referring click traffic to all of the authenticated user's bitly links."
      queryParameters: 
        unit: 
          description: |
            minute, hour, day, week or month, default: day
            Note: when unit is minute the maximum value for units is 60.
          enum: 
